        (dalos-sc:string (ref-DALOS::GOV|DALOS|SC_NAME))
        (smart-dhvault-sc:string (ref-DALOS::GOV|DHV2|SC_NAME))
        (standard-dispenser:string (ref-DSP::GOV|DSP1|SC_NAME))
        (OuroID:string (ref-DALOS::UR_OuroborosID))
        (PrimordialKosonID:string      (ref-AOZ::UR_PrimalTrueFungible 1))
        (EsothericKosonID:string       (ref-AOZ::UR_PrimalTrueFungible 2))
        (AncientKosonID:string         (ref-AOZ::UR_PrimalTrueFungible 3))
    )
    ;;1]Define IMC for Dispenser
    (ref-P|DSP::P|A_Define)
    ;;2]Reconfigure DALOS Governor, to be accesable from Dispenser, in order to use Gassless Patron for dispensing
    (ref-TS01-C1::DALOS|C_RotateGovernor patron dalos-sc
        (ref-U|G::UEV_GuardOfAny
            [
                (ref-P|DALOS::P|UR "TFT|RemoteDalosGov")
                (ref-P|DALOS::P|UR "ORBR|RemoteDalosGov")
                (ref-P|DALOS::P|UR "SWPU|RemoteDalosGov")
                (ref-P|DALOS::P|UR "TS01-A|RemoteDalosGov")
                (ref-P|DALOS::P|UR "DSP|RemoteDalosGov")
            ]
        )
    )
    ;;3]Reconfigure the SMART-DHVault Governor, to acces it fro mthe DSP Module
    (ref-TS01-C1::DALOS|C_RotateGovernor patron smart-dhvault-sc
        (ref-P|DALOS::P|UR "DSP|RemoteDalosGov")
    )
    ;;4]Toggle Mint Role for Ouro on Dispenser
    (ref-TS01-C1::DPTF|C_ToggleMintRole patron OuroID standard-dispenser true)
    ;;5]Toggle Mint Role for Primordial, Esoteric, and Ancient Kosons on Dispenser
    (ref-TS01-C1::DPTF|C_DeployAccount patron PrimordialKosonID standard-dispenser)
    (ref-TS01-C1::DPTF|C_DeployAccount patron EsothericKosonID standard-dispenser)
    (ref-TS01-C1::DPTF|C_DeployAccount patron AncientKosonID standard-dispenser)
    ;;
